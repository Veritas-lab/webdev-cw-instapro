{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "no-unused-vars": [
            "error",
            {
                "varsIgnorePattern": "^__webpack|^__unused",
                "argsIgnorePattern": "^_"
            }
        ]
    },
    "globals": {
        "module": "readonly"
    }
}
